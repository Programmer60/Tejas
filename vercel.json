{
  "version": 2,
  "name": "telemedicine-app",
  "builds": [
    {
      "src": "Client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Client/$1"
    }
  ],
  "buildCommand": "cd Client && npm install && npm run build",
  "outputDirectory": "Client/dist",
  "installCommand": "cd Client && npm install",
  "framework": "vite"
}
